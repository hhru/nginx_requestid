USE_MD5=YES
ngx_addon_name=ngx_http_requestid_module

REQUESTID_SRCS="$ngx_addon_dir/src/ngx_http_requestid_module.c"

if test -n "$ngx_module_link"; then
    ngx_module_type=HTTP_AUX_FILTER
    ngx_module_name=$ngx_addon_name
    ngx_module_srcs=$REQUESTID_SRCS

    . auto/module
else
    HTTP_AUX_FILTER_MODULES="$HTTP_AUX_FILTER_MODULES ngx_http_requestid_module"
    NGX_ADDON_SRCS="$NGX_ADDON_SRCS $REQUESTID_SRCS"
fi

